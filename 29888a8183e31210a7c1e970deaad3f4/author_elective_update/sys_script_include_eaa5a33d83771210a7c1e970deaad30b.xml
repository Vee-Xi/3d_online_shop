    initialize: function() {},

    calculatePrice: function(choice) {
        var price = 20;
        if (choice === '3_d_model_project') {
            price = 20;
        } else if (choice === '3_d_printed_item') {
            price = 100;
        }
        return price;
    },

    type: 'TEST Pricing change based on selection'
	
};]]></script>
