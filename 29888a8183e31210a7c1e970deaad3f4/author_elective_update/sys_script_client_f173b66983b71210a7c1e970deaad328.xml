        <messages/>
        <name>Autopopulate email</name>
        <order/>
        <script><![CDATA[function onChange() {
  // Get the opened_by field value
  var openedBy = g_form.getValue('opened_by');

  // Check if opened_by is populated
  if (openedBy) {
    // Get the email address using GlideRecord
    var grUser = new GlideRecord('sys_user');
    grUser.addQuery('sys_id', openedBy);
    grUser.query();
    if (grUser.next()) {
      g_form.setValue('opened_by_email', grUser.email);
    } else {
      // Handle cases where opened_by is not a valid user
      g_form.showErrorBox('opened_by_email', 'Error: Could not retrieve email for opened_by user. Please select a valid user.');
    }
  }
}]]></script>
